local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "RuniX Hub",
    Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
    LoadingTitle = "RuniX Hub",
    LoadingSubtitle = "by Grandma96523",
    Theme = "DarkBlue", -- Check https://docs.sirius.menu/rayfield/configuration/themes
 
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface
 
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil, -- Create a custom folder for your hub/game
       FileName = "RuniX Hub"
    },
 
    Discord = {
       Enabled = true, -- Prompt the user to join your Discord server if their executor supports it
       Invite = "https://discord.gg/getrunix", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
       RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
 
    KeySystem = false, -- Set this to true to use our key system
    KeySettings = {
       Title = "Untitled",
       Subtitle = "Key System",
       Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
       FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
       SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
       GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
       Key = {"Hello"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
    }
 })

 local Tab = Window:CreateTab("Player", "person-standing") -- Title, Image

 local Section = Tab:CreateSection("Player")

 Rayfield:Notify({
    Title = "Welcome to RuniX Hub!",
    Content = "Thanks for using!",
    Duration = 6.5,
    Image = "laugh",
 })

-- Sliders for WalkSpeed, JumpPower, Gravity, FOV
local WalkSpeedSlider = Tab:CreateSlider({
    Name = "Walk Speed",
    Range = {10, 500},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 16, -- Default WalkSpeed value (adjust if needed)
    Flag = "Speed", -- A flag is the identifier for the configuration file
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end,
})

local JumpPowerSlider = Tab:CreateSlider({
    Name = "Jump Power",
    Range = {10, 500},
    Increment = 1,
    Suffix = "Jump",
    CurrentValue = 50, -- Default JumpPower value (adjust if needed)
    Flag = "JumpPower", -- Unique flag for JumpPower
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end,
})

local GravitySlider = Tab:CreateSlider({
    Name = "Gravity",
    Range = {0, 400},
    Increment = 10,
    Suffix = "Gravity",
    CurrentValue = 196, -- Default Roblox gravity
    Flag = "Gravity", -- Unique flag for gravity
    Callback = function(Value)
        workspace.Gravity = Value
    end,
})

local FOVSlider = Tab:CreateSlider({
    Name = "Field of View",
    Range = {0, 120},
    Increment = 1,
    Suffix = "FOV",
    CurrentValue = 70, -- Default FOV value
    Flag = "FOV", -- Unique flag for FOV
    Callback = function(Value)
        game:GetService("Workspace").CurrentCamera.FieldOfView = Value
    end,
})

-- Reset Button to restore all values to default
local ResetButton = Tab:CreateButton({
    Name = "Reset All Values",
    Flag = "ResetButton", -- Unique flag for the reset button
    Callback = function()
        -- Reset all values to their default
        WalkSpeedSlider:Set(16) -- Default WalkSpeed
        JumpPowerSlider:Set(50) -- Default JumpPower
        GravitySlider:Set(196) -- Default Gravity
        FOVSlider:Set(70) -- Default FOV
    end,
})


local Tab = Window:CreateTab("Scripts", "scroll") -- Title, Image

local Section = Tab:CreateSection("Scripts")

local Button = Tab:CreateButton({
    Name = "Load Infinite Yield",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end,
 })

 local Button = Tab:CreateButton({
    Name = "Simple Aimbot + ESP",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Stsbba/UNC-B/refs/heads/main/Diddy"))()
    end,
 })

 local Button = Tab:CreateButton({
    Name = "NeverPatched | ChatBypass",
    Callback = function()
        loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-NeverPatched-Bypass-official-21146"))()
    end,
 })

 local Button = Tab:CreateButton({
    Name = "FISCH",
    Callback = function()
        loadstring(game:HttpGet("https://you.whimper.xyz/sources/CupPink/fisch.lua"))()
    end,
 })

 local Button = Tab:CreateButton({
    Name = "Prison Life FE",
    Callback = function()
        loadstring("\108\111\97\100\115\116\114\105\110\103\40\103\97\109\101\58\72\116\116\112\71\101\116\40\34\104\116\116\112\115\58\47\47\114\97\119\46\103\105\116\104\117\98\117\115\101\114\99\111\110\116\101\110\116\46\99\111\109\47\103\48\48\108\88\112\108\111\105\116\101\114\47\103\48\48\108\88\112\108\111\105\116\101\114\47\109\97\105\110\47\70\101\37\50\48\98\121\112\97\115\115\34\44\32\116\114\117\101\41\41\40\41\10")()
    end,
 })

 local Button = Tab:CreateButton({
    Name = "AirHub | Aimbot",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ShadowByte098/Hub/refs/heads/main/air"))()
    end,
 })

 local Button = Tab:CreateButton({
    Name = "Be Npc Or Die",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ShadowByte098/Hub/refs/heads/main/npc"))()
    end,
 })

 Rayfield:LoadConfiguration()
